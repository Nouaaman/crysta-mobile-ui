{
  "expo": {
    "name": "Crysta",
    "slug": "Crysta",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#161622",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/adaptive-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#161622"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nouaaman.Crysta"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#161622"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.nouaaman.Crysta"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "Crysta needs access to your photos to enhance and improve them with powerful tools."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
